{
  "name": "ecommerce",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "dev:css": "postcss src/styles/tailwind.css -o src/public/tailwind.css --watch",
    "build": "tsc && cp -r src/public dist/public",
    "start": "node dist/index.js",
    "test": "jest",
    "migration:generate": "npx typeorm migration:generate -d ./dist/db/index.js src/db/migrations/migration",
    "migration:run": "npx typeorm migration:run -d ./dist/db/index.js",
    "migration:revert": "npx typeorm migration:revert -d ./dist/db/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@bull-board/api": "^6.5.3",
    "@bull-board/express": "^6.5.3",
    "@elastic/elasticsearch": "^8.17.0",
    "bullmq": "^5.34.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fast-csv": "^5.0.2",
    "ioredis": "^5.4.2",
    "multer": "1.4.5-lts.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "autoprefixer": "^10.4.20",
    "jest": "^29.7.0",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}